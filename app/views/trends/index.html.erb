

<%if @user.nil?%>
<%else%>
<% @users.each do |user| %>
  <%if user.icon_image.present?%>
  <%= link_to image_tag(user.icon_image.to_s,class:'member-show-image'), "/#{user.nickname}" %>
  <%else%>
  <%= link_to image_tag("noimage.png",class:'member-show-image'), "/#{user.nickname}" %>
  <%end%>
<%end%>
<%end%>

